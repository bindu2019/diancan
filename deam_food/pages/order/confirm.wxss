.order-page {
    background: #ffffff;
    overflow: hidden;
}

.order-content {
    background: #ffffff;
    border-top: 10px solid #f8f8f8;
}

.order-title {
    padding: 10px 15px;
    color: #999999;
    font-size: 12pt;
    position: relative;
}

.order-title:before {
    content: " ";
    position: absolute;
    left: 15px;
    right: 0;
    bottom: -1px;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}

.cart-goods {
    margin: 0px 15px;
    padding: 10px 0;
    position: relative;
}

.cart-goods:before {
    content: " ";
    position: absolute;
    left: 0px;
    right: 0;
    bottom: -1px;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}

.cart-goods:last-child:before {
    border-top: 0px solid #e5e5e5;
}

.order-cart-title {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    transition-duration: 300ms;
    transition-property: background-color;
}

.order-cart-title .title {
    -webkit-box-flex: 1;
    flex: 1;
    color: #333333;
    font-size: 12pt;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.order-cart-title .count {
    color: #999999;
    font-size: 12pt;
    padding-left: 15px;
}

.order-cart-price {
    text-align: right;
    color: #333333;
    font-size: 10pt;
}

.order-cart-remark {
    color: #999999;
    font-size: 10pt;
    padding-left: 5px;
}

.order-cart-count {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    transition-duration: 300ms;
    transition-property: background-color;
    padding: 15px;
}

.order-cart-count .title {
    -webkit-box-flex: 1;
    flex: 1;
    color: #333333;
    font-size: 12pt;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.order-cart-count .price {
    font-size: 12pt;
}

.order-cart-count .price text {
    color: #fd4966;
}

.order-remark {
    background: #ffffff;
    border-top: 10px solid #f8f8f8;
    margin-bottom: 70px;
    padding: 15px;
}

.order-remark .title {
    color: #999999;
    font-size: 11pt;
}

.order-remark .remark {
    position: relative;
    margin-top: 10px;
}

.order-remark .remark textarea {
    background: #f2f2f2;
    width: calc(100% - 20px);
    font-size: 11pt;
    height: 60px;
    display: none;
}

.order-remark .remark .textarea {
    background: #f2f2f2;
    width: calc(100% - 20px);
    padding: 10px;
    font-size: 11pt;
    height: 60px;
    color: #999999;
}

.order-remark .remark .textarea.c00 {
    color: #000;
}

.order-remark .remark .count {
    position: absolute;
    bottom: 5px;
    right: 5px;
    font-size: 11pt;
    color: #999999;
    z-index: 9;
}

.cart-box {
    height: 55px;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #353535;
    line-height: 55px;
    overflow: hidden;
    z-index: 999;
}

.cart-box .icon-cart {
    margin-left: 20px;
    color: #88898f;
    font-size: 28pt;
    vertical-align: middle;
    position: relative;
}

.cart-box .cart-goodscount {
    position: absolute;
    top: 5px;
    left: 48px;
    color: #ffffff;
    background: #ff1f49 none repeat scroll 0 0;
    font-size: 10pt;
    height: 20px;
    line-height: 20px;
    padding: 0 3px;
    vertical-align: top;
    text-align: center;
    z-index: 100;
    min-width: 14px;
    border-radius: 10px;
}

.cart-box .icon-cart.hasgoods {
    color: #ffc90c;
}

.cart-box .cart-empty {
    font-size: 12pt;
    color: #88898f;
    line-height: 55px;
    margin-left: 20px;
}

.cart-price {
    line-height: 55px;
    color: #ffffff;
    font-size: 18pt;
    margin-left: 20px;
}

.submit-btn {
    float: right;
    line-height: 55px;
    padding: 0 30px;
    font-size: 13pt;
    background: #f7f7f7;
    color: #adadad;
}

.submit-btn.active {
    background: #ff9c37;
    color: #ffffff;
}

#addressInfo {
    background: #ffffff;
}

.dui-list {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    padding: 10px 15px;
    transition-duration: 300ms;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    overflow: hidden;
}

#addressInfo .dui-list {
    padding-top: 15px;
    padding-bottom: 15px;
}

#addressInfo .dui-list:after {
    content: ' ';
    position: absolute;
    left: 0;
    width: 100%;
    bottom: 0;
    height: 4px;
    background: url(data:image/png;base64,) repeat-x;
}

.dui-list-media {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -ms-flex: 0 0 auto;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    -webkit-box-lines: single;
    -moz-box-lines: single;
    flex-wrap: nowrap;
    box-sizing: border-box;
    -webkit-box-align: center;
    align-items: center;
    margin-right: 10px;
    color: #aaa;
    position: relative;
}

.dui-list-inner {
    position: relative;
    width: 100%;
    overflow: hidden;
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-flex: 1;
    flex: 1;
    display: block;
    align-self: flex-start;
}

.dui-list-inner .title {
    position: relative;
    font-size: 12pt;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    height: 24px;
    line-height: 24px;
    display: flex;
    color: #000;
}

.dui-list-inner .text {
    position: relative;
    font-size: 11pt;
    color: #666;
}

.dui-list-inner .text.no-address {
    font-size: 12pt;
    margin-top: 2px;
}

.dui-list-angle {
    position: relative;
    vertical-align: middle;
    margin-right: .6rem;
    margin-left: .5rem;
}

.dui-list .angle:after {
    content: " ";
    display: inline-block;
    transform: rotate(45deg);
    height: 6px;
    width: 6px;
    border-width: 1px 1px 0 0;
    border-color: #b2b2b2;
    border-style: solid;
    position: relative;
    top: 0px;
    margin-left: 3px;
}

.dui-list .angle:after {
    position: absolute;
    top: 50%;
    margin-top: -3px;
}

.realname {
    display: inline-block;
    max-width: 80px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding-right: 10px;
}

.mobile {
    display: inline-block;
    overflow: hidden;
}

.address {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-size: 10pt;
    line-height: 18px;
    color: #666;
}

.choose-coupon {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    transition-duration: 300ms;
    transition-property: background-color;
    padding: 15px;
}

.choose-coupon::before {
    content: " ";
    position: absolute;
    left: 15px;
    right: 15px;
    bottom: -1px;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}

.choose-coupon .title {
    -webkit-box-flex: 1;
    flex: 1;
    color: #333333;
    font-size: 12pt;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.choose-coupon .price {
    color: #fd4966;
    font-size: 11pt;
}

.choose-coupon .remark {
    font-size: 12pt;
    color: #999999;
}

.choose-coupon .remark::after {
    position: absolute;
    top: 50%;
    margin-top: -4px;
}

.choose-coupon .remark::after {
    content: " ";
    display: inline-block;
    transform: rotate(45deg);
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #b2b2b2;
    border-style: solid;
    position: relative;
    top: 0px;
    margin-left: 3px;
}

.deliver-label {
    margin-right: 5px;
    background: #ff9c37;
    color: #ffffff;
    padding: 2px 5px;
}