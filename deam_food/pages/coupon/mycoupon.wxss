page {
    background: #f0f0f0;
}

.content-empty i {
    font-size: 80pt;
}

.content-empty text {
    display: block;
}

.coupon-content {
    margin-bottom: 20px;
}

.coupon-box {
    margin: 15px 12px;
    height: 84px;
    background: #ffffff;
    border-radius: 7px;
    position: relative;
    overflow: hidden;
}

.coupon-border {
    width: 7px;
    z-index: 9;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    background: #ff9c37;
}

.status1 .coupon-border,.status2 .coupon-border {
    background: #868686!important;
}

.status1.coupon-box .coupon-con .price .coupon-price,.status2.coupon-box .coupon-con .price .coupon-price {
    color: #868686!important;
}

.coupon-dots {
    height: inherit;
    width: 6px;
    position: absolute;
    top: 0;
    left: 4px;
    z-index: 10;
}

.coupon-dots i {
    height: 6px;
    width: 6px;
    border-radius: 6px;
    background: #ffffff;
    display: block;
    margin-top: 4px;
}

.coupon-box .coupon-con {
    height: 84px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    background: #ffffff;
}

.coupon-box .coupon-con .price {
    width: 124px;
    height: 100%;
    float: left;
    position: relative;
    border-right: 1px dashed #f0f0f0;
}

.around-top,.around-bottom {
    background: #f0f0f0;
    width: 16px;
    height: 16px;
    position: absolute;
    right: -8px;
    border-radius: 16px;
}

.around-top {
    top: -8px;
}

.around-bottom {
    bottom: -8px;
}

.coupon-box .coupon-con .price .coupon-price {
    color: #ff9c37;
    font-size: 14pt;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.coupon-box .coupon-con .price .coupon-price text {
    font-size: 22pt;
}

.coupon-box .coupon-con .title {
    float: left;
    width: calc(100% - 100px - 45px);
    height: 100%;
    margin-left: 20px;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    transition-duration: 300ms;
    transition-property: background-color;
    line-height: 1;
}

.coupon-box .coupon-con .title .coupon-title {
    font-size: 13pt;
    color: #333333;
}

.coupon-box .coupon-con .title .coupon-usetime {
    font-size: 11pt;
    color: #666666;
    margin-top: 15px;
    left: 0;
}

.coupon-box .coupon-con .title .icon-tongbu {
    position: absolute;
    font-size: 16pt;
    right: 8px;
    top: 8px;
    color: #ff9c37;
    z-index: 10;
}