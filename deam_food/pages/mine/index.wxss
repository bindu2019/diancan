page {
    background: #f3f3f3;
}

.member-box {
    padding: 5px 30px 10px 30px;
    color: #ffffff;
    text-align: center;
    position: relative;
    z-index: 99;
}

.member-box .avatar {
    width: 70px;
    height: 70px;
    border-radius: 70px;
    border: rgba(255,255,255,0.3) solid 4px;
    vertical-align: middle;
}

.member-box .nickname {
    margin-top: 10px;
    font-size: 12pt;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.member-box-new {
    position: relative;
    height: auto;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    padding-bottom: 0;
}

.member-box-new .child {
    width: 30%;
    height: auto;
    padding-bottom: 20px;
    text-align: center;
}

.member-box-new .child.userinfo {
    width: 40%;
    color: #fff;
}

.member-box-new .child.userinfo .face {
    margin: auto;
}

.member-box-new .child .title {
    padding-top: 0px;
    font-size: 12pt;
    color: #fff;
    text-align: center;
}

.member-box-new .child .num {
    font-size: 11pt;
    color: #fef31f;
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.member-box-new .paybtn {
    border: 1px solid #fff;
    border-radius: 5px;
    font-size: 9pt;
    display: inline-block;
    padding: 2px 15px;
}